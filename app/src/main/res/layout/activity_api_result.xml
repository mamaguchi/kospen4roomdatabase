<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.intel.kospenmove02.ApiResultActivity">

    <TextView
        android:id="@+id/apiOutputId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="170dp"
        android:maxLength="10000"
        android:text="TextView" />

    <Button
        android:id="@+id/returnMainBtnId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/apiOutputId"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="102dp"
        android:onClick="returnMainBtnClicked"
        android:text="return to main" />

</RelativeLayout>
